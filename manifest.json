{
  "name": "Favorite Radios",
  "description" : "save your favorite radio garden stations",
  "version": "1.0",
  "permissions": ["tabs","storage"],
  "background":{
      "scripts":["background.js"],
      "persistent":false
  },
  "content_scripts": [
    {
      "matches": ["http://radio.garden/*"],
      "js": ["contentScript.js"]
    }
  ],
  "manifest_version": 2
}
